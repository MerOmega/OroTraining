{% extends '@OroUI/actions/index.html.twig' %}
{% import '@OroUI/macros.html.twig' as UI %}

{% set gridName = 'demo-entity-grid' %}
{% set pageTitle = "demos" %}

{% block navButtons %}
    {% import '@OroUI/macros.html.twig' as UI %}

    {% include '@OroImportExport/ImportExport/buttons_from_configuration.html.twig' with {
        'alias': 'demo'
    } %}

    {% if is_granted('demo_create') %}
        <div class="btn-group">
            {{ UI.addButton({
                'path': path('demo_create'),
                'entity_label': 'testing'
            }) }}
        </div>
    {% endif %}
{% endblock %}
